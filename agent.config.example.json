{
  "llm": {
    "provider": "openai",
    "model": "gpt-4o",
    "temperature": 0.7,
    "maxTokens": 2000
  },
  "server": {
    "port": 3000,
    "baseDir": "data"
  },
  "agent": {
    "maxSteps": 10,
    "timeout": 300000
  },
  "embedding": {
    "provider": "hash",
    "dimension": 256,
    "alpha": 0.6
  },
  "sandbox": {
    "enabled": true,
    "maxWriteSize": 1048576,
    "fileTypeBlacklist": [
      ".exe",
      ".sh",
      ".bat",
      ".cmd",
      ".ps1",
      ".dll",
      ".so"
    ],
    "rateLimit": {
      "maxCalls": 60,
      "windowMs": 60000
    }
  },
  "feishu": {
    "enabled": false,
    "appId": "cli_xxx",
    "appSecret": "replace-with-your-secret",
    "baseUrl": "https://open.feishu.cn",
    "timeoutMs": 20000,
    "maxRetries": 3,
    "pollIntervalMs": 120000
  },
  "mineru": {
    "enabled": false,
    "apiKey": "ak_xxx_or_sk_xxx",
    "baseUrl": "https://mineru.net/api/v4",
    "uidToken": "",
    "timeoutMs": 20000,
    "maxRetries": 3,
    "pollIntervalMs": 3000,
    "maxPollAttempts": 120,
    "defaultModelVersion": "pipeline"
  }
}
